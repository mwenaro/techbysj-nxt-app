{
  "name": "techbysj-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "devapp": "next dev",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "lint": "next lint",
    "format": "prettier --check --ignore-path .gitignore .",
    "format:fix": "prettier --write --ignore-path .gitignore .",
    "postinstall": "husky install",
    "husky": {
      "hooks": {
        "pre-commit": "lint-staged"
      }
    },
    "lint-staged": {
      "*": "prettier --write"
    }
  },
  "dependencies": {
    "@types/node": "20.2.3",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.3",
    "firebase": "^9.22.1",
    "formik": "^2.4.0",
    "framer-motion": "^10.12.16",
    "mongoose": "^7.2.2",
    "next": "13.4.3",
    "next-gallery": "^1.2.1",
    "nodemailer": "^6.9.3",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.9.0",
    "react-image-gallery": "^1.2.11",
    "react-scroll": "^1.8.9",
    "react-toastify": "^9.1.3",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@types/react-image-gallery": "^1.2.0",
    "@types/react-scroll": "^1.8.7",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8"
  }
}
